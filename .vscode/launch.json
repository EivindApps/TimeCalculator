        {
            "command": "npm run test:unit",
            "name": "Run vue-cli-service test:unit",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npm run serve",
            "name": "Run vue-cli-service serve",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/src",
            "sourceMapPathOverrides": {
                "meteor://ðŸ’»app/*": "${workspaceFolder}/*",
                "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
                "webpack:///src/*": "${webRoot}/*"
